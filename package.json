    "sass-loader": "^8.0.2",
    "typescript": "~3.8.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "@vue/prettier",
      "@vue/prettier/@typescript-eslint"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
        "ecmaFeatures": {
          "jsx": false
        }
    },
    "rules": {
      "no-debugger": 1,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/ban-ts-ignore": 0,
      "@typescript-eslint/no-use-before-define": 0
    }
  },
  "prettier": {
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
